<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="message" class="springhellotutorial.Message">
        <property name="message" value="Hello everybody!"/>
    </bean>

    <bean id="messageToYou" class="springhellotutorial.inheritance.MessageToYou">
        <property name="message" value="Hello everybody!"/>
        <property name="messageToYou" value="eeeeeeeeeee"/>
    </bean>

</beans>